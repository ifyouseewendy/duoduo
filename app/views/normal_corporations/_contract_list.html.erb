<ul>
  <% contract_files.each do |cf| %>
    <li>
      <span><%= cf.contract_identifier %></span>
      <span>( <%= link_to '下载', cf.contract.url %> 
        <%= form_tag(contract_file_path(cf), method: :delete, class: 'form_tag') do %>
          <%= link_to "删除",  "#", :onclick=>"$(this).closest('form').submit()" %>
        <% end %>
         )
      </span>
    </li>
  <% end %>
</ul>

