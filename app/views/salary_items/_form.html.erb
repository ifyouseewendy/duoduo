<%= semantic_form_for SalaryItem.new, url: { action: :create } do |f| %>
  <%= f.inputs do %>
    <%= f.input :staff_name, as: :string %>
    <%= f.input :staff_identity_card, as: :string, label: "身份证号（非必须）" %>
    <%= f.input :salary_deserve, as: :number %>
  <%- end %>
  <%= f.actions %>
<%- end %>
